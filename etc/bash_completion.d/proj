_proj() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    FILES=/home/jprieto/projects/*
    opts=""
    for entry in $FILES
    do
      bname=`basename "$entry"`
      opts+="$bname "
    done

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
    return 0

}
complete -F _proj proj